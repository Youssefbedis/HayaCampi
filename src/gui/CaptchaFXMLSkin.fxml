<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="captcha" alignment="CENTER" type="VBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CaptchaFXMLSkin">
  <children>
    <Label fx:id="captchaQuestion" text="Label" />
    <TextField id="captchaQuestion" fx:id="captchaInput" prefWidth="200.0" promptText="Solution" />
    <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonPressed" text="OK" />
  </children>
  <stylesheets>
    <URL value="@CaptchaSkin.css" />
  </stylesheets>
</fx:root>
